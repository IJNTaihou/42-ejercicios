//Ejercicios de estructuras selectivas.Hoja1.Ejercicio 19
#include <conio.h>
#include <iostream>

using namespace std;


int main()
{
	//ENTRADA
	float notaAlumno;

	cout << "Ingrese la nota del alumno: ";
	cin >> notaAlumno;

	//LOGICA
	if (notaAlumno >= 1 && notaAlumno < 5) {
		cout << "Necesita estudiar" << endl;
	}
	else {
		if (notaAlumno >= 5 && notaAlumno < 13) {
			cout << "Jalado" << endl;
		}
		else {
			if (notaAlumno >= 13 && notaAlumno < 15) {
				cout << "Raspando" << endl;
			}
			else {
				if (notaAlumno >= 15 && notaAlumno < 20) {
					cout << "Sobresaliente" << endl;
				}
				else {
					cout << "Matricula de honor" << endl;
				}
			}
		}
	}

	_getch();
}
